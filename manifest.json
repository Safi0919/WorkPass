{
  "manifest_version": 3,
  "name": "Job Eligibility Checker",
  "version": "1.0",
  "description": "Check if job descriptions accept international students.",
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  }
}
